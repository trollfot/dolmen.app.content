<div class="content-actions" tal:condition="viewlet/actions">
  <h5>Content actions:</h5>
  <ul>
    <tal:actions repeat="action viewlet/actions">
      <li tal:attributes="class python:action['action'] == viewlet.selected
			               and 'tab selected' or 'tab'">
	<a href="" tal:content="action/title"
	   tal:attributes="href string:${viewlet/contexturl}/${action/action};
			   title action/title;"
	   />
      </li>
    </tal:actions>
  </ul>
</div>
